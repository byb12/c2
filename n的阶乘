#include<stdio.h>
int main()
{  int n,i;
   int s=1;
   for(n=1;n<4;n++)
   s*=n;
   printf("%d",s);
   return 0;
} 
//递归函数
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int facl (int n)
{
	if (n<=1)
	{
		return 1;
	}
	else
		return n*facl(n-1);
}
int main()
{
	int n=3;
	int z=facl(n);
	printf("%d", z);
	return 0;
}
